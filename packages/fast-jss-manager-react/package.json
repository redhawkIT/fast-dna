{
  "name": "@microsoft/fast-jss-manager-react",
  "description": "React HOC for managing component JSS stylesheets",
  "author": {
    "name": "Microsoft",
    "email": "fwq@microsoft.com"
  },
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast-dna.git"
  },
  "readme": "README.md",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepare": "tsc -p ./tsconfig.json",
    "test": "npm run tslint && npm run unit-test",
    "tslint": "tslint -c tslint.json ./src/**/*.ts{,x}",
    "tslint:fix": "tslint -c tslint.json ./src/**/*.ts{,x} --fix",
    "tslint:watch": "node build/tslint-watch.js",
    "unit-test": "jest",
    "unit-test:watch": "jest --watch && npm-watch"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 69,
        "functions": 80,
        "lines": 80,
        "statements": -10
        }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "devDependencies": {
    "@microsoft/fast-tslint-rules": "^1.0.4",
    "@types/jest": "^22.1.2",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "jest": "^22.3.0",
    "react": "^16.0.0",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^22.0.4",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "dependencies": {
    "@microsoft/fast-jss-manager": "^1.0.0",
    "hoist-non-react-statics": "^2.3.1",
    "jss": "^9.8.0",
    "jss-preset-default": "^4.3.0",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.6.0",
    "uuid": "^3.2.1"
  }
}
