{
  "name": "@microsoft/fast-permutator",
  "description": "Creates all possible permutations of a component based on its JSON Schema.",
  "sideEffects": false,
  "version": "2.2.0",
  "author": {
    "name": "Fluent Web",
    "url": "https://discord.gg/FcSNfg4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/fast-dna.git"
  },
  "main": "index.js",
  "scripts": {
    "prepare": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prettier": "prettier --config ../../.prettierrc --write '**/*.js'",
    "prettier:diff": "prettier --config ../../.prettierrc '**/*.js' --list-different",
    "test": "npm run eslint && npm run unit-tests",
    "eslint": "eslint src/**",
    "unit-tests": "jest --maxWorkers=4"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 89,
        "branches": 78,
        "functions": 96,
        "lines": 85
      }
    },
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "!<rootDir>/node_modules/lodash-es"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/))\\.(js?)$"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "jest": "^23.6.0",
    "lodash-es": "^4.17.10",
    "prettier": "1.14.3"
  },
  "peerDependencies": {
    "lodash-es": "^4.17.10"
  }
}
